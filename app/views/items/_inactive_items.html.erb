<% unless @inactive_items.empty? %>
	<br>
    <ul class="list-group">
      <li class="list-group-item bg-danger text-white">Inactive Items</li>
      <% @inactive_items.each do |item| %>
        <li class="list-group-item"><%= link_to item.name, item_path(item) %></li>
      <% end %>
    </ul>
<% end %>

<%= link_to "New Item", new_item_path %>